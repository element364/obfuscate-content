{
  "name": "obfuscate-content",
  "version": "1.2.2",
  "type": "module",
  "description": "obfuscateContent is a helper function to replace all alphabetical characters in a string with analogs",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "unpkg": "./dist/index.umd.js",
  "types": "./src/index.d.ts",
  "scripts": {
    "build": "microbundle --no-generateTypes",
    "dev": "microbundle --no-generateTypes watch",
    "test": "vitest",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/element364/obfuscate-content.git"
  },
  "keywords": [
    "content",
    "obfuscation"
  ],
  "author": "element364",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/element364/obfuscate-content/issues"
  },
  "homepage": "https://github.com/element364/obfuscate-content#readme",
  "devDependencies": {
    "microbundle": "^0.15.1",
    "vitest": "^0.33.0"
  }
}
